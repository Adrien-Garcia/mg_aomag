<?xml version="1.0"?>
<config>
    <modules>
        <Addonline_JQuery>
            <version>0.0.1</version>
        </Addonline_JQuery>
    </modules>

    <frontend>   
        <layout>
            <updates>
                <jquery>
                    <file>jquery.xml</file>
                </jquery>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <addonline translate="title">
                                        <title>Addonline config</title>
                                        <sort_order>100</sort_order>
                                    </addonline>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>	
    </adminhtml> 
</config>